# 44. FACET  skos:broader - 1
sip-concept:FACET a skos:Concept ;
    rdfs:label "Facet"@en ;
    skos:definition "Format specification of a component's content when reported in a data or metadata set."@en ;
    skos:notation "urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=SDMX:CROSS_DOMAIN_CONCEPTS(2.0).FACET" ;
    skos:inScheme sip-concept:cog ;
