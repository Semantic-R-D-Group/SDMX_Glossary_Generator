    skos:notation "urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=SDMX:CROSS_DOMAIN_CONCEPTS(2.0).MEASURE" ;
    skos:inScheme sip-concept:cog ;
    rdfs:comment "In a SDMX data set, the instance of a measure is often called an observation."@en ;
    skos:broader sip-concept:DSD .

# 49. MEMBER_SEL  skos:broader - 1
sip-concept:MEMBER_SEL a skos:Concept ;
